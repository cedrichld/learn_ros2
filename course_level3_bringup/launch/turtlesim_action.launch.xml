<launch>
    <node pkg="rclcpp_components" exec="component_container_mt" name="move_turtle_container" />
    <executable cmd="ros2 component load /move_turtle_container final_proj3_cpp 
        final_proj3_cpp::TurtleSpawnKillClientNode" output="screen" />
    <executable cmd="ros2 component load /move_turtle_container final_proj3_cpp 
        final_proj3_cpp::MoveTurtleServerNode" output="screen" />
    <executable cmd="ros2 component load /move_turtle_container final_proj3_cpp 
        final_proj3_cpp::MoveTurtlebot3ServerNode" output="screen" />

    <node pkg="turtlesim" exec="turtlesim_node" />

    <node pkg="lifecycle_py" exec="lifecycle_node_manager">
        <param name="managed_node_name" value="turtle_spawn_kill_client" />
    </node>

    <set_env name="TURTLEBOT3_MODEL" value="waffle"/>
    <include file="$(find-pkg-share turtlebot3_gazebo)/launch/turtlebot3_world.launch.py" />
    <!-- <node pkg="final_proj3_cpp" exec="turtle_spawn_kill" />
    <node pkg="final_proj3_cpp" exec="move_turtle" /> -->
</launch>